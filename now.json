{
  "version": 2,
  "name": "markdown",
  "builds": [ {
      "src": "*.css",
      "use": "@now/static"
    },
    {
      "src": "**/*.md",
      "use": "@now/md",
      "config": {
        "title": "Nodejs labs",
        "language": "en",
        "meta": [{ "name": "description", "content": "Example markdown builder deployed to ZEIT NOW"}],
        "css": "/style.css"
      }
    }
  ]
}
